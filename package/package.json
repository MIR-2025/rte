{
  "name": "rte-rich-text-editor",
  "version": "1.0.23",
  "description": "A standalone, embeddable rich text editor. One script tag. Zero dependencies. Works everywhere.",
  "main": "rte.js",
  "module": "rte.esm.js",
  "browser": "rte.js",
  "unpkg": "rte.js",
  "jsdelivr": "rte.js",
  "types": "rte.d.ts",
  "exports": {
    ".": {
      "import": "./rte.esm.js",
      "require": "./rte.js",
      "default": "./rte.js"
    }
  },
  "files": [
    "rte.js",
    "rte.esm.js",
    "rte.d.ts",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "rich-text-editor",
    "wysiwyg",
    "editor",
    "contenteditable",
    "text-editor",
    "html-editor",
    "embeddable",
    "standalone",
    "no-dependencies",
    "rte"
  ],
  "author": "phpMyDEV, LLC",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MIR-2025/rte.git"
  },
  "bugs": {
    "url": "https://github.com/MIR-2025/rte/issues"
  },
  "homepage": "https://rte.whitneys.co"
}
